{
  "name": "champz-frontend-v2",
  "private": true,
  "version": "1.6.6",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000 --host",
    "build-prod": "tsc && vite build",
    "build-beta": "tsc && vite build --mode beta",
    "build-dev": "tsc && vite build --mode development",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:tsc": "tsc --noemit",
    "lint:prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "preview": "vite preview",
    "prepare": "husky"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.12",
    "@mui/material": "^5.15.12",
    "@rainbow-me/rainbowkit": "^2.0.1",
    "@tanstack/react-query": "^5.25.0",
    "@tanstack/react-query-devtools": "^5.25.0",
    "@tanstack/react-router": "^1.19.2",
    "@tanstack/router-devtools": "^1.19.2",
    "axios": "^1.6.7",
    "jotai": "^2.7.0",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-cookie": "^7.1.0",
    "react-dom": "^18.2.0",
    "react-full-screen": "^1.1.1",
    "react-string-replace": "^1.1.1",
    "viem": "2.x",
    "wagmi": "^2.5.7"
  },
  "devDependencies": {
    "@tanstack/router-vite-plugin": "^1.19.3",
    "@types/react": "^18.2.56",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "add": "^2.0.6",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^9.0.11",
    "npm-run-all": "^4.1.5",
    "prettier": "3.2.5",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vite-plugin-pwa": "^0.19.3",
    "vite-tsconfig-paths": "^4.3.1",
    "yarn": "^1.22.21"
  }
